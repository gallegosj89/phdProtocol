@article{wang_improving_2025,
	title = {Improving fruit variety classification using near-infrared spectroscopy and deep learning techniques},
	volume = {140},
	copyright = {https://www.elsevier.com/tdm/userlicense/1.0/},
	issn = {0889-1575},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0889157525000572},
	doi = {10.1016/j.jfca.2025.107243},
	abstract = {Accurate identification of different fruit varieties is essential for suppliers, processors and consumers, particularly during grading, breeding and harvest timing. Traditional manual recognition is inefficient, while image-based methods often struggle to differentiate different varieties with similar appearances. Additionally, analysing the internal chemical composition of fruits is complex and requires specialised expertise, making the process cumbersome. Near-infrared spectroscopy (NIRS) has gained increasing attention for fruit variety identification due to its ability to rapidly assess internal fruit characteristics. However, NIRS data often exhibit high redundancy and low variability, which make effective analysis challenging, thus requiring advanced machine learning techniques. In recent years, deep learning has demonstrated promising results in fruit classification using NIRS. However, NIRS data contain important positional information, which convolutional neural networks (CNNs) often struggle to capture effectively due to their limited ability to process continuity and spatial relationships. To address this challenge, we propose a novel approach that combines gated recurrent units (GRUs) with a twostream network architecture. GRUs are adept at learning and memorising the relationships between the start and end positions of spectral sequences, while the two-stream network integrates the strengths of both CNNs and GRUs, enabling the model to simultaneously capture local features and sequence information. Additionally, we incorporate a simple attention mechanism and a new feature module to enhance the model’s ability to represent features. Experimental results on NIRS data for 10 mango varieties and 4 blueberry cultivars demonstrate that our model achieves a 15 \%–20 \% higher classification accuracy than traditional machine learning methods and other fruit classification approaches. When combined with XGBoost, the model’s classification accuracy reached 95 \% for mangoes and 96 \% for blueberries, underscoring its robustness and adaptability. This method holds significant potential for applications such as fruit grading, breeding and harvesting, offering a promising path to improving both efficiency and accuracy for suppliers, processors and consumers.},
	language = {en},
