@article{noutfia_comprehensive_2022,
	title = {Comprehensive {Characterization} of {Date} {Palm} {Fruit} ‘{Mejhoul}’ ({Phoenix} dactylifera {L}.) {Using} {Image} {Analysis} and {Quality} {Attribute} {Measurements}},
	volume = {13},
